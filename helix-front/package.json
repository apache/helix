    "ngx-vis": "micahstubbs/ngx-vis#micahstubbs/424/ivy-distribution",
    "propagating-hammerjs": "2",
    "request": "^2.81.0",
    "rxjs": "^7.5.5",
    "timsort": "0.3.0",
    "tslib": "^2.3.1",
    "uuid": "8",
    "vis": "^4.21.0",
    "vis-data": "6.5.1",
    "vis-util": "4.0.0",
    "xss": "1",
    "zone.js": "^0.11.5"
  },
