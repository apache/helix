{
  "name": "helix-front",
  "version": "1.2.1",
  "author": "Vivo Xu <vxu@linkedin.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/helix.git"
  },
  "keywords": [
    "helix",
    "ui"
  ],
  "scripts": {
    "ng": "ng",
    "build": "rm -rf dist && mkdir dist && ng build --configuration production && tsc -p server",
    "start": "tsc -p server && concurrently -r \"ng serve -pc proxy.conf.json\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "npm run build && node dist/server/app.js",
    "test": "ng test",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.html --ignore-pattern node_modules/,dist/",
    "lint:fix": "eslint . -c .eslintrc.js --ext .ts,.html --ignore-pattern node_modules/,dist/ --fix",
    "lint:errors:only": "yarn lint --quiet",
    "e2e": "ng e2e",
    "smoke": "ng lint && ng test --sr && npm run build",
    "type:check": "tsc --project tsconfig.json --noEmit",
    "type:check:stats": "yarn type:check --diagnostics true",
    "type:check:watch": "yarn type:check --watch",
    "type:check:watch:stats": "sh scripts/type:check:watch:stats.sh",
    "type:check:watch:stats:verbose": "yarn type:check --watch --diagnostics true"
  },
  "dependencies": {
    "@angular/animations": "^13.3.3",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "^13.3.3",
    "@angular/core": "^13.3.3",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "^13.3.3",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "^13.3.3",
    "@angular/platform-browser-dynamic": "^13.3.3",
    "@angular/router": "^13.3.3",
    "@egjs/hammerjs": "^2.0.17",
    "@swimlane/ngx-charts": "^20.1.0",
    "@swimlane/ngx-datatable": "swimlane/ngx-datatable#master",
    "@swimlane/ngx-graph": "^8.0.0",
    "angulartics2": "^12.0.0",
    "body-parser": "^1.17.2",
    "component-emitter": "1.3.0",
    "core-js": "^3.22.1",
    "d3-shape": "^1.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "keycharm": "^0.2.0",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.4",
