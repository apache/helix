    "vis": "^4.21.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "9.1.15",
    "@angular/compiler-cli": "9.1.13",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.120",
    "@types/node": "^12.11.1",
    "@types/request": "^2.0.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.5.2",
