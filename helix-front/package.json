    "@typescript-eslint/eslint-plugin-tslint": "^5.17.0",
    "@typescript-eslint/parser": "5.11.0",
    "concurrently": "^3.5.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
